(deflisten volume :initial "?"
                  `scripts/getVolume`)

(defwidget volume []
  (box :class "square"
       :valign "start"
       :style "margin-right: 0.4em;"
    (eventbox :class "event-listener"
    (label :style "padding-left: 0.4em; padding-right: 0.4em;"
           :halign "center"
           :text volume)))
)
